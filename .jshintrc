{
	/* http://www.jshint.com/docs/options/ */
	/* Enforcing options */
	"camelcase"	: true,
	"curly"		: true,
	"eqeqeq"	: true,
	"es3"		: true,
	"forin"		: true,
	"freeze"	: true,
	"immed"		: true,
	"indent"	: true,
	"latedef"	: true,
	"newcap"	: true,
	"noarg"		: true,
	"nonbsp"	: true,
	"nonew"		: true,
	"plusplus"	: true,
	"quotmark"	: "single",
	"undef"		: true,
	"unused"	: true,
	"strict"	: true,
	"trailing"	: true,
	"maxparams"	: 4,
	"maxdepth"	: 4,
	"maxstatements":24,
	"maxlen"	: 80,
	/* Environments */
	"browser"	: true,
	"jquery"	: true,
	"nonstandard":true
}
